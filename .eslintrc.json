{
  "extends": "next/core-web-vitals",
  "env": {
    "browser": true,
    "es2022": true
  },
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [
        ".eslintrc.{js,cjs}"
      ],
      "parserOptions": {
        "sourceType": "script"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "@typescript-eslint/no-misused-promises": 0,
    "@typescript-eslint/strict-boolean-expressions": 0
  }
}